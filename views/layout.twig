<!DOCTYPE HTML>
<html lang="{{ reevio.language }}">
<head>
    <script type="text/javascript">$('iframe').wrap('<div class="video" />');</script>
    <meta charset="utf-8">
    {% include "html_header.twig" %}
    {% if title is defined %}
    <title>{{ title }} - {{ reevio.blogtitle }}</title>
    {% else %}
    <title>{{ reevio.blogtitle }}</title>
    {% endif %}
</head>

<body>
    <nav> <!-- Page navigation -->
        <a href="{{ path('feed') }}" class="feed"></a>
    </nav>
    <div class="clear_head"></div>
    <div id="container">
        <div id="main" role="main">
            {% block content %}Nothing to see here{% endblock %}
        </div>
        {% include "sidebar.twig" %}
        <div class="clear"></div>
    </div>
    {% include "footer.twig" %}
</body>
</html>
